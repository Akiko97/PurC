<hvml target="html">
    <head>
        <init as="buttons" against="letters">
        [
            { "letters": "7", "class": "number" },
            { "letters": ".", "class": "number" },
            { "letters": "%", "class": "c_blue percent" },
            { "letters": "=", "class": "c_yellow equal" },
        ]
        </init>
    </head>
    <body>
        <div>
            $DATA.count($buttons)
        </div>
        <erase on="$buttons" at="[1]"/>
        <div>
            $buttons[1].letters
        </div>
        <div>
            $buttons[1].class
        </div>
    </body>
</hvml>
