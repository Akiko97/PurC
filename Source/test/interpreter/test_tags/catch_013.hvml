<!DOCTYPE hvml>
<hvml target="html" lang="en">
    <head>
    </head>

    <body>
        <div>
            <differ>
                <catch for=`EntityNotFound`>
                    <update on="$@" at="attr.entity_not_found" to="displace" with="matched" />
                </catch>
            </differ>
        </div>
        <catch for=`InvalidValue`>
            <update on="$@" at="attr.invalid_value" to="displace" with="matched" />
        </catch>
    </body>
</hvml>



