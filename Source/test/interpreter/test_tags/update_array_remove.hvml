#!/usr/bin/purc

# update array : remove

# RESULT: 'zh_Hans'

<!DOCTYPE hvml SYSTEM 'v: MATH'>
<hvml target="html" lang="en">
    <head>
    </head>

    <body>
        <init as="locales">
            [ "zh_CN", "zh_Hans" ]
        </init>

        <div id="msg">
        </div>

        <update on="$locales" to="remove" at=[0] />

        <catch for=`NotAllowed`>
            <update on="#msg" at="textContent" with="NotAllowed" />
            <exit with "NotAllowed" />
        </catch>

        <exit with $locales[0] />
    </body>
</hvml>



